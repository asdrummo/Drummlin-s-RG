<%= form_tag(:controller => 'public', :action=> 'attempt_login') do %>

		<div style="display:inline">
			<%= label_tag(:email) %>:
			<%= text_field_tag(:email) %>
		</div>
		<div style="display:inline">
			<%= label_tag(:password) %>:
			<%= password_field_tag(:password) %>
		</div>

			<%= submit_tag("login") %>

	<% end %>